process {
  withName: 'PUBLISH_INPUT_DATASET|SENTIEON_ALIGNMENT|SENTIEON_DNASCOPE|PREPROCESS_VCF|CUSTOM_SPLIT_QUERY_TABLE_CHR|CUSTOM_SPLIT_BAM_CHR|CUSTOM_BAM_PILEUP_FILTER|CUSTOM_RSCRIPT_SOMATICSNP_FILTER_1_CELL_LEVEL_CREATE_TABLE|CUSTOM_RSCRIPT_SOMATICSNP_FILTER_2_CELL_LEVEL_CONCAT_FILTER_TABLE|CUSTOM_GET_LIST_POS_GROUP|CUSTOM_BAM_GROUP_PILEUP_CHR|CUSTOM_RSCRIPT_SOMATICSNP_FILTER_3_GROUPLEVEL_PROCESS_PILEUP_SAMPLE|CUSTOM_RSCRIPT_SOMATICSNP_FILTER_4_CREATE_TABNR_TABNV|SEQUOIA|SUBSET_VCF_VARIANTS|SENTIEON_DRIVER_METRICS|SENTIEON_DRIVER_COVERAGEMETRICS|CUSTOM_DATA_PROCESSING|CUSTOM_REPORT|REPORT_VERSIONS|MULTIQC' {
    queue = 'long'
  }
  withName: "SENTIEON_.*" {
    maxForks = 15
    errorStrategy = 'retry'
  }
  withName: "CUSTOM_RSCRIPT_SOMATICSNP_FILTER_4_CREATE_TABNR_TABNV" {
    errorStrategy = { task.exitStatus in [9,104,130,134,137,138,139,140,143] ? 'retry' : 'terminate' }
    memory = { 15.GB * task.attempt }
  }
  withName: "CUSTOM_RSCRIPT_SOMATICSNP_FILTER_3_GROUPLEVEL_PROCESS_PILEUP_SAMPLE" {
    errorStrategy = { task.exitStatus in [9,104,130,134,137,138,139,140,143] ? 'retry' : 'terminate' }
    memory = { 7.GB * task.attempt }
  }
  withName: 'SEQUOIA' {
    errorStrategy = { task.exitStatus in [9,104,130,134,137,138,139,140,143] ? 'retry' : 'terminate' }
    queue = 'basement'
  }
}